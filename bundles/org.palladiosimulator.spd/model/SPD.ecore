<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="spd" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/1.0"
    nsPrefix="spd">
  <eClassifiers xsi:type="ecore:EClass" name="ReactiveScalingPolicy" eSuperTypes="#//ScalingPolicy">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="A reactive scaling policy contains a scaling trigger that determines whether an adjustment should be made and an adjustment that defines the performed adjustment."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="adjustmentType" lowerBound="1"
        eType="#//adjustments/AdjustmentType" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="scalingTrigger" lowerBound="1"
        eType="#//triggers/ScalingTrigger" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SPD" eSuperTypes="platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//core/entity/Entity">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="nameInvariant noSameTargetGroup"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="The root elements that consists of all scaling policies under analysis for a given cloud application. The SPD is an Entity (PCM), it has a unique identifier and it has a name.  "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="scalingPolicies" lowerBound="1"
        upperBound="-1" eType="#//ScalingPolicy" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The set of scaling policies under analysis for the given cloud application model."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targetGroups" lowerBound="1"
        upperBound="-1" eType="#//targets/TargetGroup" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ScalingPolicy" abstract="true" eSuperTypes="platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//core/entity/Entity">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="policyNameInvariant"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="A scaling policy determines the complete information for scaling a parituclar target. Each ScalingPolicy is an Entity (PCM), it has a unique identifier and it has a name.  "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="active" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="policyConstraints" upperBound="-1"
        eType="#//constraints/policy/PolicyConstraint" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targetGroup" lowerBound="1"
        eType="#//targets/TargetGroup"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ModelBasedScalingPolicy" eSuperTypes="#//ScalingPolicy">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="A model-based scaling policy contains a model that determines both whether any scaling should occur and the type of adjustment by itself."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="model" lowerBound="1" eType="#//models/BaseModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="adjustment" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
  </eClassifiers>
  <eSubpackages name="targets" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Targets/1.0"
      nsPrefix="targets">
    <eClassifiers xsi:type="ecore:EClass" name="TargetGroup" abstract="true" eSuperTypes="platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//core/entity/Entity">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A TargetGroup defines a management group in SPD. It is both uniqely identified as well as it has a name, thus it extends from the Entity class of the PCM. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="targetConstraints" upperBound="-1"
          eType="#//constraints/target/TargetConstraint" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ElasticInfrastructure" eSuperTypes="#//targets/TargetGroup">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The ElasticInfrastructure target groups resource containers on which components are allocated. The ElasticInfrastructure allows the definition of scaling policies for the whole infrastructure which is a common use case covered by the prominent cloud providers. For example, AWS allows the definition of an Autoscaling Group where EC2 instances are automatically provisioned."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="PCM_ResourceEnvironment"
          eType="ecore:EClass platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//resourceenvironment/ResourceEnvironment"
          changeable="false" volatile="true" transient="true" unsettable="true" derived="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="unit" eType="ecore:EClass platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//resourceenvironment/ResourceContainer"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ServiceGroup" eSuperTypes="#//targets/TargetGroup">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The ServiceGroup groups a set of components that are load balanced and can be horizontally replicated. Upon replication the load balancer distributes the load to the new replicas according to the predefined load balancing strategy."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="unitAssembly" eType="ecore:EClass platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//core/composition/AssemblyContext">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The unitAssembly is used to point to the ServiceGroup in PCM. It is used also for disinguishing between different service groups. A prerequisite is that the unit assembly is already connected in a Service Group structure in PCM. "/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CompetingConsumersGroup" eSuperTypes="#//targets/TargetGroup">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The CompetingConsumersGroup represents a set of elements that consume messages from a channel/queue asynchronously. In SPD and Palladio it is represented as a set of assembly context that deplete a queue modelled through a PassiveResource. The intention of use behind CompetingConsumersGroup is to represent services that asynchronously process workload by fetching messages from a queue. For understanding the pattern itself one can read https://www.enterpriseintegrationpatterns.com/patterns/messaging/CompetingConsumers.html.  "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="unitAssembly" eType="ecore:EClass platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//core/composition/AssemblyContext">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The unitAssembly of the CompetingConsumersGroup identifies the AssemblyContext which exists in the System model and which shall be replicated upon scaling out. Furthermore, the unit assembly is the one which will exist throughout the simulation independent of the adaptations. "/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="adjustments" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Adjustments/1.0"
      nsPrefix="adjustments">
    <eClassifiers xsi:type="ecore:EClass" name="AdjustmentType" abstract="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="An AdjustmentType determines how the target group is adjusted upon the firing of a trigger."/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="RelativeAdjustment" eSuperTypes="#//adjustments/AdjustmentType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The RelativeAdjustment denotes that the group is adjusted relatively to the current number of elements. &#xA;The RelativeAdjustment contains two parameters: the percentageGrowthValue and the minAdjustmentValue.&#xA;The percentageGrowthValue determines the change (increase/decrease) of the current capacity as a percentage value.&#xA;The minAdjustmentValue determines the minimal change of the current capacity."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="percentageGrowthValue"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
          defaultValueLiteral="100">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The percantage value of adjustment e.g., a value of 10 denotes that 10% should be added to the existing capacity."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minAdjustmentValue" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt" defaultValueLiteral="0">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="A minimum adjustment value in case the percentage is 0. "/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="AbsoluteAdjustment" eSuperTypes="#//adjustments/AdjustmentType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The AbsoluteAdjustment denotes that the group is adjusted to a goal value."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="goalValue" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt" defaultValueLiteral="0">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The goalValue determines the target number of elements for a particular group, e.g., a value 5 means that the group will have 5 elements."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="StepAdjustment" eSuperTypes="#//adjustments/AdjustmentType">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The StepAdjustment denotes that the group is adjusted by adding or removing a fixed amount of elements."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="stepValue" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt" defaultValueLiteral="0">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The stepValue describes how many elements in the group should be added or removed. For example, a vallue of -1 determines that one element should be removed from the group."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="constraints" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Constraints/1.0"
      nsPrefix="constraints">
    <eClassifiers xsi:type="ecore:EClass" name="AbstractConstraint" abstract="true"
        eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="TemporalConstraint" abstract="true"
        eSuperTypes="#//constraints/AbstractConstraint"/>
    <eClassifiers xsi:type="ecore:EClass" name="StateBasedContraint" abstract="true"
        eSuperTypes="#//constraints/AbstractConstraint"/>
    <eSubpackages name="policy" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Constraints/Policy/1.0"
        nsPrefix="policy">
      <eClassifiers xsi:type="ecore:EClass" name="PolicyConstraint" abstract="true"
          eSuperTypes="#//constraints/AbstractConstraint"/>
      <eClassifiers xsi:type="ecore:EClass" name="IntervalConstraint" eSuperTypes="#//constraints/TemporalConstraint #//constraints/policy/PolicyConstraint">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The IntervalConstraint identifies fixed intervals in which a policy enacts adjustments. Contrary to the CooldownConstraint, the IntervalConstraint predefines the enactment of the policy in time. "/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="offset" lowerBound="1"
            eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The offset determines a period of time from which the interval constraint should begin. "/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="intervalDuration" lowerBound="1"
            eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The duration in which no enactment by the policy occurs. "/>
          </eAnnotations>
        </eStructuralFeatures>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="repeat" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
            defaultValueLiteral="false">
          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="The repeat property determines whether the interval constraint should repeat throughout the simulation or is applied once, which is the default case (false)."/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="CooldownConstraint" eSuperTypes="#//constraints/TemporalConstraint #//constraints/policy/PolicyConstraint">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="The CoolDown constraint defines a quiescence period in which the target group is not enacted by the policy. &#xA;In addition one can specify the maximum number of scaling operations that can occur in the defined quiescence period. &#xA;Contrary to the IntervalConstraint, the CooldownConstraint determines the future enactment of the policy after an adjustment has happened."/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="cooldownTime" lowerBound="1"
            eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxScalingOperations"
            lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
            defaultValueLiteral="0"/>
      </eClassifiers>
    </eSubpackages>
    <eSubpackages name="target" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Constraints/Target/1.0"
        nsPrefix="target">
      <eClassifiers xsi:type="ecore:EClass" name="TargetConstraint" abstract="true"
          eSuperTypes="#//constraints/AbstractConstraint"/>
      <eClassifiers xsi:type="ecore:EClass" name="TargetGroupSizeConstraint" eSuperTypes="#//constraints/target/TargetConstraint #//constraints/StateBasedContraint">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="minSize" lowerBound="1"
            eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxSize" lowerBound="1"
            eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ThrashingConstraint" eSuperTypes="#//constraints/TemporalConstraint #//constraints/target/TargetConstraint">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Thrashing constraint is used to constraint the thrashing of resources i.e. increase and decrease of resources. The constraint is defined by the minimum amount of time where no two decicions with adjustments in two oposite directions can occur. "/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="minimumTimeNoThrashing"
            eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      </eClassifiers>
    </eSubpackages>
  </eSubpackages>
  <eSubpackages name="triggers" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Triggers/1.0"
      nsPrefix="triggers">
    <eClassifiers xsi:type="ecore:EClass" name="ScalingTrigger" abstract="true" eSuperTypes="platform:/plugin/de.uka.ipd.sdq.identifier/model/identifier.ecore#//Identifier">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The ScalingTrigger is a core concept in SPD. It entails both the information that is fetched from the simulation and models as well as the type of analysis that is performed on that information. It is uniquely identified as it extends from Idenfier (PCM)."/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="BaseTrigger" abstract="true" eSuperTypes="#//triggers/ScalingTrigger">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A BaseTrigger is a class of ScalingTrigger that works on a Stimulus (that entails the information gathered from the environment) and an ExpectedValue. Once the Stimulus 'matches' the ExpectedValue the trigger fires and an adjustment to the model is made. The matching of Stimulus with an ExpectedValue is determined by the subclasses. This can entail simple analysis through relational operators or more advanced transformation/aggregation of the Stimulus and the ExpectedValue. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="stimulus" lowerBound="1"
          eType="ecore:EClass ../../org.palladiosimulator.spd.stimulus/model/stimulus.ecore#//Stimulus"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="expectedValue" lowerBound="1"
          eType="#//triggers/expectations/ExpectedValue" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ComposedTrigger" eSuperTypes="#//triggers/ScalingTrigger">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A ComposedTrigger composes two or more ScalingTriggers through a logical operator (i.e., AND, OR, XOR). This enables the defintion of composed triggers which encapsulate more advanced conditions on the state to fire the trigger. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="scalingtrigger" lowerBound="2"
          upperBound="-1" eType="#//triggers/ScalingTrigger" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="logicalOperator" eType="#//triggers/LogicalOperator"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="LogicalOperator">
      <eLiterals name="AND"/>
      <eLiterals name="OR" value="1"/>
      <eLiterals name="XOR" value="2"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SimpleFireOnValue" eSuperTypes="#//triggers/BaseTrigger">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The SimpleFireOnValue trigger the most simplistic BaseTrigger that works on the fed stimulus through a relational operator with an expected value. In case 'LessThen' is specified then the the trigger will fire upon stiumuls &lt; expectedValue. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="relationalOperator" eType="#//triggers/RelationalOperator"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="RelationalOperator">
      <eLiterals name="LessThan"/>
      <eLiterals name="GreaterThan" value="1"/>
      <eLiterals name="EqualTo" value="2"/>
      <eLiterals name="LessThanOrEqualTo" value="3"/>
      <eLiterals name="GreaterThanOrEqualTo" value="4"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="TrendPattern">
      <eLiterals name="Increasing"/>
      <eLiterals name="Decreasing" value="1"/>
      <eLiterals name="NonIncreasing" value="2" literal="NonIncreasing"/>
      <eLiterals name="NonDecreasing" value="3" literal="NonDecreasing"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SimpleFireOnTrend" eSuperTypes="#//triggers/BaseTrigger">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The SimpleFireOnTrend trigger is a more advanced trigger that aggregates values and determines a trend and fires whenever this trend is as an expectedTrend value. "/>
      </eAnnotations>
    </eClassifiers>
    <eSubpackages name="expectations" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Triggers/Expectations/1.0"
        nsPrefix="expectations">
      <eClassifiers xsi:type="ecore:EClass" name="ExpectedValue" abstract="true"/>
      <eClassifiers xsi:type="ecore:EClass" name="ExpectedPrimitive" abstract="true"
          eSuperTypes="#//triggers/expectations/ExpectedValue">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="There are three different primitives in the context of triggers: percentages, time, and count."/>
        </eAnnotations>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="NoExpectation" eSuperTypes="#//triggers/expectations/ExpectedValue"/>
      <eClassifiers xsi:type="ecore:EClass" name="ExpectedPercentage" eSuperTypes="#//triggers/expectations/ExpectedPrimitive">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ExpectedCount" eSuperTypes="#//triggers/expectations/ExpectedPrimitive">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="count" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ExpectedTime" eSuperTypes="#//triggers/expectations/ExpectedPrimitive">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ExpectedTrend" eSuperTypes="#//triggers/expectations/ExpectedValue">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="trend" eType="#//triggers/TrendPattern"/>
      </eClassifiers>
    </eSubpackages>
  </eSubpackages>
  <eSubpackages name="models" nsURI="http://palladiosimulator.org/ScalingPolicyDefinition/Adjustments/Models/1.0"
      nsPrefix="models">
    <eClassifiers xsi:type="ecore:EClass" name="BaseModel" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="interval" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="1.0"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="initalIntervalDelay"
          eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble"
          defaultValueLiteral="0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QThresholdsModel" eSuperTypes="#//models/LearningBasedModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="input" eType="ecore:EClass ../../org.palladiosimulator.spd.stimulus/model/stimulus.ecore#//Stimulus"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="utilizationStimulus"
          eType="ecore:EClass ../../org.palladiosimulator.spd.stimulus/model/stimulus.ecore#//Stimulus"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="learningRate" lowerBound="1"
          eType="#//models/PercentDouble" defaultValueLiteral="0.1"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="epsilon" lowerBound="1"
          eType="#//models/PercentDouble" defaultValueLiteral="0.05"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="RandomModel" eSuperTypes="#//models/BaseModel">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minAdjustment" lowerBound="1"
          eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxAdjustment" lowerBound="1"
          eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="probability" lowerBound="1"
          eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble"
          defaultValueLiteral="1.0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ImprovedQLearningModel" eSuperTypes="#//models/LearningBasedModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="input" lowerBound="1"
          eType="ecore:EClass ../../org.palladiosimulator.spd.stimulus/model/stimulus.ecore#//Stimulus"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="epsilon" lowerBound="1"
          eType="#//models/PercentDouble" defaultValueLiteral="0.05"/>
